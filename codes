push**************
git init
git status
git add .
git commit -m "hello world"
git branch -M main
git remote add origin link
git fetch origin
git pull origin main
git commit 
git push -u origin main
pull****************
mkdir git-demo
cd git_demo
pwd
mkdir changes
cd changes
pwd
git init 
git pull link
git add .
git commit -m "changes made"
git remote add origin link
git remote -v
git push -u origin master
docker***************
FROM python:3.11-slim

WORKDIR /app

COPY hello.py /app

CMD ["python", "hello.py"]
rename dockerfile.txt dockerfile
docker  build -t python312-hari .
docker login
docker tag python312/harichandanas/python312-hari
docker push harichandanas/python312-hari
docker run python312-hari
